<div id="screenInfo">
  <h2>Screen Information</h2>
  <table class="table responsive">
    <tbody>
      <tr>
        <td>Screen Name</td>
        <td>{{{ workflowData.screenName }}}</td>
      </tr>
      <tr>
        <td>Library</td>
        <td>{{{ library }}}</td>
      </tr>
      <tr>
        <td>Screen Stage</td>
        <td>{{{ workflowData.screenStage }}}</td>
      </tr>
      <tr>
        <td>Image Dates</td>
        <td>{{{ plateInfo.vendorPlate.creationdate }}}</td>
      </tr>
      <tr>
        <td>Junk</td>
        <td>{{{ workflowData.isJunk }}}</td>
      </tr>
      <tr>
        <td>Comments</td>
        <td>{{{ workflowData.data.comments }}}</td>
      </tr>
      <tr>
        <td>Worm Strain</td>
        <td>{{{ workflowData.data.wormStrain }}}</td>
      </tr>
      <tr>
        <td>Chemical Plate</td>
        <td>{{{ workflowData.data.chemicalPlate }}}</td>
      </tr>
      <tr>
        <td>Chemical Concentration</td>
        <td>{{{ workflowData.data.chemicalConcentration }}}</td>
      </tr>
      <tr>
        <td>% DMSO</td>
        <td>{{{ workflowData.data.dmso }}}</td>
      </tr>
      <tr>
        <td>Incubation Time</td>
        <td>{{{ workflowData.data.incubationTime }}}</td>
      </tr>
      <tr>
        <td>Incubation Temperature (C)</td>
        <td>{{{ workflowData.data.incubationTemp }}}</td>
      </tr>
      <tr>
        <td>OD Reading Date</td>
        <td>{{{ workflowData.data.odReadingDate }}}</td>
      </tr>
    </tbody>
  </table>
</div>

<div id="enviraPlateGallery">
  <h2>Plate Gallery</h2>
  {{{enviraGallery}}}
</div>
